    186              // Put code here to handle Power-On reset
    187            }
    188            else if ( rib == RESETEX )
    189            {
    190              // Put code here to handle External reset
    191            }
    192            else if ( rib == RESETWD )
    193            {
    194              // Put code here to handle WatchDog reset
    195            }
